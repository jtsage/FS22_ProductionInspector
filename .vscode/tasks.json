	"version": "2.0.0",
	"tasks": [
	  {
		"label": "Refresh ProductionInspector ZIP",
		"type": "shell",
		"command": "node ./makeZip.js",
		"windows": {
		  "command": "node .\\makeZip.js"
		},
		"options": {
			"cwd" : "./.build/",
		},
		"group": "build",
		"presentation": {
		  "reveal": "always",
		  "panel": "new"
		}
	  }
	]
  }